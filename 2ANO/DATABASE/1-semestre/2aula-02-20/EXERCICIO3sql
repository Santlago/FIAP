SET SERVEROUTPUT ON;
DECLARE
    VALOR_AQUISICAO NUMBER:= &VALOR;
    VALOR_FINANCIADO NUMBER := &FINANCIADO;
    VALOR_PARCELA NUMBER;
    TOTAL NUMBER;
BEGIN
    -- PRINT
    DBMS_OUTPUT.PUT_LINE('VALOR DO CARRO r$: ' || VALOR_AQUISICAO);
    DBMS_OUTPUT.PUT_LINE('VALOR FINANCIADO' || (valor_aquisicao - VALOR_FINANCIADO));
    DBMS_OUTPUT.PUT_LINE('VALOR DAS PARCELAS r$: ' || (valor_aquisicao - VALOR_FINANCIADO)*1.3);
    
    
END;