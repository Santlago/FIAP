
SET SERVEROUTPUT ON;

DECLARE

V_MOSTRAR_NOME VARCHAR2(30) := 'SANTOS SANTOS';
-- V_MEUNOME VARCHAR2(30) := &NOME;
V_RA NUMBER := 44554555666;
MOSTRAR_NOME VARCHAR(30);

BEGIN

    INSERT INTO ALUNO VALUES (V_RA, V_MOSTRAR_NOME);
    UPDATE ALUNO SET NOME = 'BRENO' WHERE RA = V_RA;
    COMMIT;
    SELECT NOME INTO MOSTRAR_NOME FROM ALUNO WHERE RA = V_RA;
    dbms_output.PUT_LINE(MOSTRAR_NOME);

END;

